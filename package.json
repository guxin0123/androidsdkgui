{
  "name": "androidsdkgui",
  "productName": "androidsdkgui",
  "version": "0.0.1",
  "description": "Android SDK GUI Tool",
  "main": "src/index.ts",
  "scripts": {
    "build-theme": "node-sass src/theme.scss src/theme.css",
    "start": "npm run build-theme && electron-forge start",
    "make": "npm run build-theme && electron-forge make --all",
    "lint": "tslint src",
    "test": "mocha --compilers ts:ts-node/register"
  },
  "keywords": [],
  "author": "kuluna",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "androidsdkgui"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "androidsdkgui"
      }
    }
  },
  "dependencies": {
    "@angular/animations": "^4.3.5",
    "@angular/cdk": "^2.0.0-beta.8",
    "@angular/common": "^4.3.5",
    "@angular/compiler": "^4.3.5",
    "@angular/core": "^4.3.5",
    "@angular/flex-layout": "^2.0.0-beta.8",
    "@angular/forms": "^4.3.5",
    "@angular/http": "^4.3.5",
    "@angular/material": "^2.0.0-beta.8",
    "@angular/platform-browser": "^4.3.5",
    "@angular/platform-browser-dynamic": "^4.3.5",
    "@angular/router": "^4.3.5",
    "electron-compile": "^6.4.1",
    "electron-devtools-installer": "^2.0.1",
    "electron-settings": "^3.1.1",
    "reflect-metadata": "^0.1.9",
    "tslib": "^1.7.1",
    "zone.js": "^0.8.16"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/electron-settings": "^3.0.0",
    "@types/mocha": "^2.2.41",
    "electron-forge": "~3.1.1",
    "electron-prebuilt-compile": "1.7.5",
    "mocha": "^3.5.0",
    "node-sass": "^4.5.3",
    "ts-node": "^3.3.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2"
  }
}
